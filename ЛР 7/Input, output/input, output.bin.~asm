mov cx, 3   ;10111001  00000011  00000000
next:
mov ah, 1h  ;10110100  00000001
int 21h     ;11001101  00100001
mov dl, al  ;10001010  11010000
add dl, 1   ;10000000  11000010  00000001
mov ah, 2h  ;10110100  00000010
int 21h     ;
loop next   ;11100010  11110001




